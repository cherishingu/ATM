<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="EvaluationView">
        <attr name="is_horizontal_center" format="boolean"/>
        <attr name="is_vertical_center" format="boolean"/>

        <attr name="left_padding" format="dimension"/>
        <attr name="top_padding" format="dimension"/>

    </declare-styleable>

    <declare-styleable name="PasswordInputView">
        <attr name="pivBorderColor" format="color"/>
        <attr name="pivBorderWidth" format="dimension"/>
        <attr name="pivBorderRadius" format="dimension"/>
        <attr name="pivPasswordColor" format="color"/>
        <attr name="pivPasswordWidth" format="dimension"/>
        <attr name="pivPasswordRadius" format="dimension"/>
        <attr name="pivPasswordLength" format="integer"/>
    </declare-styleable>

    <declare-styleable name="TimeCountView">
        <attr name="backgroundcolor" format="color"/>
        <attr name="text" format="string"/>
        <attr name="maxNum" format="integer"/>
    </declare-styleable>


    <declare-styleable name="ImageViewWithText">
        <attr name="textcolor" format="color"/>
        <attr name="textvalue" format="string"/>
        <attr name="text_size" format="dimension"/>
        <attr name="imgsrc" format="integer"/>
    </declare-styleable>
</resources>